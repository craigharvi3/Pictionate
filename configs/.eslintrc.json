{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true
	},
	"globals": {
		"$": true,
		"jQuery": true,
		"$aw2": true,
		"module": true,
		"setTimeout": true,
		"setInterval": true,
		"location": true,
		"encodeURIComponent": true,
		"toString": true
	},
	"plugins": [
		"react"
	],
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"rules": {
	}
}
